{
  "include": ["src"],
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2019",
    "lib": ["esnext", "dom"],
    "declaration": false,
    "strict": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "outDir": "dist",
    "rootDir": "src"
  }
}

