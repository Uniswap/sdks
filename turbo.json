{
  "pipeline": {
    "lint": {
      "inputs": [
        "sdks/*/src/**.ts",
        "sdks/*/src/**.tsx"
      ],
      "outputs": [],
      "cache": true
    },
    "build": {
      "inputs": [
        "yarn.lock",
        "**/package.json",
        "sdks/*/src/**.ts",
        "sdks/*/src/**.tsx"
      ],
      "outputs": [
        "sdks/*/dist/**"
      ],
      "cache": true,
      "dependsOn": ["^build"]
    },
    "test": {
      "dependsOn": [
        "build"
      ],
      "inputs": [
        "sdks/*/dist/**",
        "sdks/*/test/**",
        "sdks/*/src/**"
      ],
      "outputs": [],
      "cache": true
    },
    "release": {
      "dependsOn": [],
      "cache": false
    },
    "typecheck": {
      "dependsOn": [],
      "outputs": [],
      "cache": true
    }
  }
}
