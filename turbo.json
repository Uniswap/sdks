{
  "pipeline": {
    "lint": {
      "inputs": [
        "sdks/*/src/**.ts",
        "sdks/*/src/**.tsx"
      ]
    },
    "build": {
      "dependsOn": ["^build"],
      "inputs": [
        "yarn.lock",
        "**/package.json",
        "sdks/*/src/**.ts",
        "sdks/*/src/**.tsx"
      ],
      "outputs": [
        "sdks/*/dist/**"
      ]
    },
    "test": {
      "dependsOn": [
        "build"
      ],
      "inputs": [
        "sdks/*/dist/**"
      ]
    },
    "release": {
      "dependsOn": ["^release"]
    },
    "typecheck": {
      "dependsOn": []
    }
  }
}
