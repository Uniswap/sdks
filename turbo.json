{
  "tasks": {
    "lint": {
      "inputs": [
        "sdks/*/src/**.ts",
        "sdks/*/src/**.tsx",
        "!sdks/uniswapx-sdk"
      ]
    },
    "build": {
      "inputs": [
        "yarn.lock",
        "**/package.json",
        "sdks/*/src/**.ts",
        "sdks/*/src/**.tsx",
        "!sdks/uniswapx-sdk"
      ],
      "outputs": [
        "sdks/*/dist/**"
      ]
    },
    "test": {
      "dependsOn": [
        "build"
      ],
      "inputs": [
        "sdks/*/dist/**",
        "!sdks/uniswapx-sdk"
      ]
    },
    "release": {
      "dependsOn": []
    },
    "typecheck": {
      "dependsOn": []
    }
  }
}
